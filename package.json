{
  "name": "@antfly/sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Antfly API - works in both frontend and backend",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:ts": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generate": "npx openapi-typescript ../antfly/openapi.yaml -o ./src/antfly-api.d.ts",
    "prepublishOnly": "npm run test:ts && npm run lint && npm run test && npm run build",
    "example:node": "tsx examples/node-example.ts"
  },
  "keywords": [
    "antfly",
    "sdk",
    "typescript",
    "api",
    "client",
    "database",
    "vector",
    "search"
  ],
  "author": "Antfly Contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/antfly/antfly-sdk-ts.git"
  },
  "bugs": {
    "url": "https://github.com/antfly/antfly-sdk-ts/issues"
  },
  "homepage": "https://github.com/antfly/antfly-sdk-ts#readme",
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "jest": "^29.7.0",
    "openapi-typescript": "^7.9.1",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "openapi-fetch": "^0.14.0"
  },
  "engines": {
    "node": ">=18"
  }
}

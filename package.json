{
  "name": "@antfly/sdk",
  "version": "0.0.4",
  "description": "TypeScript SDK for Antfly API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "node --experimental-vm-modules node_modules/.bin/jest",
    "test:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
    "test:ts": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "generate": "npx openapi-typescript ../antfly/openapi.yaml -o ./src/antfly-api.d.ts && npx openapi-typescript bleve-query-openapi.yaml --client fetch -o ./src/bleve-query.d.ts",
    "prepublishOnly": "npm run test:ts && npm run lint && npm run test && npm run build",
    "example:node": "tsx examples/node-example.ts"
  },
  "keywords": [
    "antfly",
    "sdk",
    "typescript",
    "api",
    "client",
    "database",
    "vector",
    "search"
  ],
  "author": "Antfly Contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antflydb/antfly-ts.git"
  },
  "bugs": {
    "url": "https://github.com/antflydb/antfly-ts/issues"
  },
  "homepage": "https://github.com/antflydb/antfly-ts#readme",
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/jest": "^25.1.0",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "jest": "^28.1.0",
    "openapi-typescript": "^5.4.2",
    "prettier": "^3.6.2",
    "ts-jest": "^28.0.5",
    "tsup": "^8.1.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "openapi-fetch": "^0.14.0"
  },
  "engines": {
    "node": ">=18"
  }
}
